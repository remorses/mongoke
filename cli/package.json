{
    "name": "goke",
    "_": "[bump]",
    "version": "2.0.24",
    "description": "Goke cli",
    "keywords": [],
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts",
    "bin": "./dist/main.js",
    "mocha": {
        "require": "tests/init.js",
        "spec": "tests/**.ts",
        "timeout": 9999999999
    },
    "files": [
        "dist",
        "src",
        "esm"
    ],
    "scripts": {
        "test": "NODE_ENV=test mocha --colors --exit",
        "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
        "dev": "yarn compile && node dist",
        "build": "tsc && mkdir -p dist/web-ui/ && cp -r ../web-ui/out/ dist/web-ui/",
        "postinstall": "go-npm install",
        "preuninstall": "go-npm uninstall"
    },
    "goBinary": {
        "name": "goke-server",
        "path": "./bin",
        "url": "https://github.com/remorses/mongoke/releases/download/v{{version}}/mongoke_{{version}}_{{platform}}_{{arch}}.tar.gz"
    },
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/mocha": "^7.0.2",
        "@types/node": "^14.0.11",
        "assert": "^2.0.0",
        "dotenv": "^8.2.0",
        "mocha": "^7.2.0",
        "sucrase": "^3.15.0",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@types/yargs": "^15.0.5",
        "go-npm": "^0.1.9",
        "tslib": "^2.0.0",
        "yargs": "^15.3.1"
    }
}
